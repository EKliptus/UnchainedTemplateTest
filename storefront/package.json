{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "SUPPRESS_ENV_ERRORS=1 next build",
    "dev:local": "UNCHAINED_ENDPOINT=http://localhost:4010/graphql next dev",
    "dev": "next dev",
    "format": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet .",
    "test": "npm run lint && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-server-micro": "^2.14.2",
    "apollo-server-plugin-response-cache": "^0.5.0",
    "apollo-upload-client": "^13.0.0",
    "dotenv-extended": "^2.8.0",
    "geoip-country": "^4.0.19",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.8",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^11.1.0",
    "next-with-apollo": "^5.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^5.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.0.5",
    "@storybook/addon-links": "6.0.5",
    "@storybook/addons": "6.0.5",
    "@storybook/react": "6.0.5",
    "@types/node": "^14.0.4",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.0.5",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
